<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="horizontal">

	<ImageView
        android:id="@+id/imgkode"
		android:layout_height="36dp"
        android:padding="2dp"
		android:src="@drawable/ij"
        android:contentDescription="@null"
		android:layout_width="30dp" />

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:weightSum="1">
	    
       <TableRow
        android:layout_width="fill_parent"
		   android:layout_height="17dp">
	       
	       <TextView
		        android:id="@+id/ltxnopel"
	           	android:textColor="#323EAD"
			    android:textSize="13sp"
			    android:textStyle="bold"
			   android:layout_height="15dp"
			   android:layout_width="100dp" />

		   <TextView
			   android:text="@string/Nama"
                   android:id="@+id/ltxnama"
			   android:textColor="#323EAD"
                android:textSize="13sp"
                   android:layout_weight="1"
                   android:textStyle="bold"
			   android:layout_height="15dp"
			   android:layout_width="190dp" />

		   <TextView
			   android:layout_height="wrap_content"
               android:id="@+id/ltxalong"
			   android:visibility="gone"
			   android:layout_width="20dp" />


	   </TableRow>

		<TableRow
			android:layout_height="match_parent"
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_weight="0.33"
			tools:ignore="InefficientWeight">

			<TextView
                android:id="@+id/ltxalamat"
                android:textColor="#de1507"
                android:textSize="13sp"
                android:textStyle="bold"
				android:text="@string/alamaat"
				android:layout_height="match_parent" />

		</TableRow>

		<TableRow
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:visibility="gone">

			<TextView
				android:id="@+id/ltxtadminstrasi"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:text="TextView"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxMinim"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxDigit"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtarif4"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtarif3"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtarif2"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtrid"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxnomet"
				tools:ignore="HardcodedText" />

			<TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:id="@+id/ltxawal"
				tools:ignore="HardcodedText" />

			<TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:id="@+id/ltxakhir"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxkubik"
				tools:ignore="HardcodedText" />

			<TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:id="@+id/ltxnominal"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxpiutang"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtotal"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxbad_cd"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxangsuran"
				tools:ignore="HardcodedText" />

			<TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:id="@+id/ltxtarif1"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtarif_cd"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtarif_nohp"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtabul"
				tools:ignore="HardcodedText" />

			<TextView
				android:text="@string/trltx"
				android:layout_width="wrap_content"
				android:layout_height="5dp"
				android:id="@+id/ltxtrid"
				android:visibility="gone"
				tools:ignore="DuplicateIds" />

			<TextView
				android:layout_width="230dp"
				android:layout_height="5dp"
				android:id="@+id/ltxalat"
				android:visibility="gone" />

			<TextView
				android:id="@+id/ltxtgalctt"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="TextView" />

		</TableRow>

	</LinearLayout>

</LinearLayout>